container_commands:
    01_migrations:
        command: "source /opt/python/run/venv/bin/activate && python3 /manage.py makemigrations GolfRound"
        command: "source /opt/python/run/venv/bin/activate && python3 /manage.py makemigrations accounts"
        command: "source /opt/python/run/venv/bin/activate && python3 /manage.py makemigrations Courses"
        command: "source /opt/python/run/venv/bin/activate && python3 /manage.py makemigrations golf_trip"
        command: "source /opt/python/run/venv/bin/activate && python3 /manage.py makemigrations bookie"
        command: "source /opt/python/run/venv/bin/activate && python3 manage.py makemigrations"
        leader_only: true
    02_migrate:
        command: "source /opt/python/run/venv/bin/activate && python3 manage.py migrate --noinput"
    03_collectstatic:
        command: "source /opt/python/run/venv/bin/activate && python3 manage.py collectstatic --noinput"